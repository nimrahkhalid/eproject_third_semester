@model List<user>
<section>
	<div class="gap">
		<div class="container">
			<div class="row">
				<div class="col-lg-12">
					<div id="page-contents align-content-between" class="row merged20">
						<div class="col-lg-9">
							<div class="main-wraper align-center">
								<h4 class="main-title"><i class=""><svg class="feather feather-users" stroke-linejoin="round" stroke-linecap="round" stroke-width="2" stroke="currentColor" fill="none" viewBox="0 0 24 24" height="18" width="18" xmlns="../../../www.w3.org/2000/svg.html"><path d="M17 21v-2a4 4 0 0 0-4-4H5a4 4 0 0 0-4 4v2" /><circle r="4" cy="7" cx="9" /><path d="M23 21v-2a4 4 0 0 0-3-3.87" /><path d="M16 3.13a4 4 0 0 1 0 7.75" /></svg></i> Add friends</h4>
								<div class="row col-xs-6">
											@foreach (var user in Model)
											{
									<div class="col-lg-3 col-md-4 col-sm-4">
												<div class="group-box">
												<figure><img src="@("~/user_image/"+user.user_image)" asp-append-version="true"></figure>
												<a title="" href="#">@user.user_username</a>
												<button>Add Friend</button>
												</div>
									</div>
											}
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>


		</div>
	</div>
</section>
